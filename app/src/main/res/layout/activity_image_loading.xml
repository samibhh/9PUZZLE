<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/lottieBG2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:lottie_rawRes="@raw/menubg"
        tools:ignore="MissingConstraints" />
    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/lottie2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:lottie_rawRes="@raw/puzzle2"
        tools:ignore="MissingConstraints" />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="45dp"
        android:id="@+id/lay"
        >


        <ImageView
            android:id="@+id/imageView"
            android:layout_width="206dp"
            android:layout_height="307dp"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="107dp"

            android:background="#FFFFFF"
            app:srcCompat="@drawable/logo" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/gallery"
            android:layout_width="220dp"
            android:layout_height="59dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="103dp"
            android:layout_marginTop="384dp"
            android:layout_marginEnd="108dp"
            android:background="#F7AA1C"
            android:text="GALLERY GENRATION"
            android:textColor="#FFFFFF"
            tools:ignore="TextContrastCheck" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/backward"
            android:layout_width="220dp"
            android:layout_height="59dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="103dp"
            android:layout_marginTop="576dp"
            android:layout_marginEnd="108dp"
            android:background="#8B348B"
            android:text="RETURN"
            android:textColor="#FFFFFF"
            tools:ignore="TextContrastCheck" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/app"
            android:layout_width="220dp"
            android:layout_height="59dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="101dp"
            android:layout_marginTop="481dp"
            android:layout_marginEnd="110dp"
            android:background="#279D9A"
            android:text="APP GENERATION"
            android:textColor="#FFFFFF"
            tools:ignore="TextContrastCheck" />

        <ImageView
            android:id="@+id/srcimage"
            android:layout_width="293dp"
            android:layout_height="160dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="0dp"
            android:layout_marginTop="-30dp"
            android:layout_marginEnd="411dp"
            android:layout_marginBottom="761dp"
            tools:visibility="gone" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="328dp"
            android:layout_height="188dp"
            android:layout_below="@+id/imageView"
            android:layout_alignBottom="@+id/gallery"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="37dp"
            android:layout_marginTop="-105dp"
            android:layout_marginEnd="46dp"
            android:layout_marginBottom="53dp"
            app:srcCompat="@drawable/kll" />
    </RelativeLayout>
</RelativeLayout>